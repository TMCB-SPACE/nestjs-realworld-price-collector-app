{
  "name": "@tmcb/nestjs-realworld-price-collector-app",
  "version": "0.0.0-development",
  "keywords": [],
  "description": "Nest.js test tooling",
  "author": "TED Vortex <ted.vortex@gmail.com>",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/TMCB-SPACE/nestjs-realworld-price-collector-app#readme",
  "bugs": {
    "url": "https://github.com/TMCB-SPACE/nestjs-realworld-price-collector-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TMCB-SPACE/nestjs-realworld-price-collector-app.git"
  },
  "engines": {
    "node": ">= 21",
    "npm": ">= 10"
  },
  "scripts": {
    "preinstall": "npx @tmcb/check-engines",
    "prestart": "npx @tmcb/check-engines",
    "push": "npx @tmcb/conventional-commit",
    "types:auto-add": "npx typesync",
    "types:auto-check": "npm run types:auto-add -- --dry",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:k6:transactions": "./k6 run -e K6_API_URL=http://localhost:3001/v1 ./test/k6/transaction.spec.ts",
    "compose:up": "docker compose up --remove-orphans -d",
    "compose:down": "docker compose down --remove-orphans",
    "compose:destroy": "docker compose down --rmi local --remove-orphans -v",
    "compose:refresh": "npm run compose:destroy && npm run compose:up"
  },
  "dependencies": {
    "@depyronick/nestjs-clickhouse": "^2.0.2",
    "@fastify/express": "^2.3.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^6.12.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/platform-fastify": "^10.3.1",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/typeorm": "^10.0.1",
    "cache-manager-ioredis-yet": "^1.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "glob": "^10.3.10",
    "nestjs-pino": "^4.0.0",
    "pg": "^8.11.3",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "ts-enum-util": "^4.0.2",
    "typeorm": "^0.3.19",
    "typeorm-transactional-cls-hooked": "^0.1.21"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^4.4.0",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.1",
    "@tmcb/check-engines": "^1.0.1",
    "@tmcb/conventional-commit": "^1.0.0",
    "@types/eslint": "^8.56.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/k6": "~0.48.0",
    "@types/node": "^20.11.6",
    "@types/pg": "^8.10.9",
    "@types/source-map-support": "^0.5.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "typesync": "^0.12.1"
  }
}
